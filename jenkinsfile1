pipeline {
    agent any

    stages {
        stage('Dev-Stage') {
            steps {
                echo 'Hello Dev-Stage'
                build quietPeriod: 3, job: 'test1'
            }
        }
        stage('Test-Stage') {
            steps {
                echo 'Hello Test Env'
                build quietPeriod: 3, job: test2'
            }
        }
        stage('Prod-Stage') {
            steps {
                echo 'Hello Prod Env'
                build quietPeriod: 3, job: 'test33'
            }
        }
    }
}
